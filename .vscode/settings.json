{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "cSpell.words": [
        "Anony",
        "Bson",
        "Millis",
        "randomstring",
        "Retryable",
        "saka",
        "subquery",
        "upserted",
        "uuidv",
        "Zhang"
    ],
    "cSpell.ignorePaths": [
        "package.json",
        "**/.gitlab-ci.yml",
        "**/package-lock.json",
        "**/yarn.lock",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/objects/**",
        ".vscode",
        ".vscode-insiders"
    ],
    "npm.packageManager": "yarn"
}
